<br>
<div class="container-fluid" id="wrapper" >
<%= form_for  @people do |form| %>
  <%= form.label :name, "Ingreso de nueva Persona" %>

  <div class="field">
    <%= form.label :rut %>
    <%= form.text_field :rut %>
  </div>


  <div class="field">
    <%= form.label :name %>
    <%= form.text_field :name, id: :name %>
  </div>


  <div class="field">
    <%= form.label :name2 %>
    <%= form.text_field :name2, id: :name2 %>
  </div>

  <div class="field">
    <%= form.label :last_name1 %>
    <%= form.text_field :last_name1, id: :last_name1 %>
  </div>

  <div class="field">
    <%= form.label :last_name2 %>
    <%= form.text_field :last_name2, id: :last_name2 %>
  </div>

  <div class="field">
    <%= form.label :photo %>
    <%= form.text_field :photo, id: :photo %>
  </div>

  <div class="field">
    <%= form.label :e_civil %>
    <%= form.text_field :e_civil, id: :e_civil %>
  </div>


  <div class="field">
    <%= form.label :profesion %>
    <%= form.text_field :profesion, id: :profesion %>
  </div>


  <div class="field">
    <%= form.label :nacionality %>
    <%= form.text_field :nacionality, id: :nacionality %>
  </div>


  <div class="field">
    <%= form.label :passport %>
    <%= form.text_field :passport, id: :passport %>
  </div>




 <%= form.submit "Guardar" %>

<% end %>
</div>
