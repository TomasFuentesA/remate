<p id="notice"><%= notice %></p>

<h1>Realty Domainfiles</h1>

<table>
  <thead>
    <tr>
      <th>File name</th>
      <th>Observation</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @realty_domainfiles.each do |realty_domainfile| %>
      <tr>
        <td><%= realty_domainfile.file_name %></td>
        <td><%= realty_domainfile.observation %></td>
        <td><%= link_to 'Show', realty_domainfile %></td>
        <td><%= link_to 'Edit', edit_realty_domainfile_path(realty_domainfile) %></td>
        <td><%= link_to 'Destroy', realty_domainfile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Realty Domainfile', new_realty_domainfile_path %>
