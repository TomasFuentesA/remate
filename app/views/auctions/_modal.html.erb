
 <div class="row justify-content-md-center">
  <div class="card" style="width: 18rem;">
    <div class="card-body">
        <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#exampleModal" >Añadir nueva Propiedad</button>
        <br>
        <br>
        <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#Modaljudgement" >Añadir nuevo Juicio</button>
    </div>
  </div>
</div>

<div class="col">
  <div class="row justify-content-md-center">
    <div class="modal fade bd-example-modal-lg" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
      <div class="modal-dialog" role="document">
        <div class="modal-content" style="width: 130% !important;">
          <div class="modal-header">
            <h5 class="modal-title" id="exampleModalLabel">Ingreso de Propiedad:</h5>
            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
              <span aria-hidden="true">&times;</span>
            </button>
          </div>
          <div class="modal-body">
            <div style="
              height: 110px;
              overflow: auto;
              width: 580px;
              margin-left: 20px;">
              <%= @auctionnotice.auction %>
            </div>
            <%= render partial: "realties/form_modal" , :locals => { :item => @auctionnotice } %>

          </div>
          <div class="modal-footer">
            <button type="button" class="btn btn-secondary" data-dismiss="modal" id="cancelar">Cancelar</button>
          </div>
        </div>

      </div>
    </div>

  </div>
</div>
