<table id="myTable" class="table">
  <thead>
    <tr>
      <th>Seleccionar Propiedad</th>

    </tr>
  </thead>

  <tbody>
    <% @realties.each do |realty| %>

      <tr>
        <td><%= realty.street + " " + (realty.number_unit).to_s + " " + realty.unit_estate+ " " + (realty.commune_id).to_s %></td>

        <td>
          <%= link_to 'Show', realty %>
          <%= link_to 'Edit', edit_realty_path(realty) %>
          <%= link_to 'Destroy', realty, method: :delete, data: { confirm: 'Are you sure?' } %>

      </tr>
    <% end %>
  </tbody>
</table>

<br>

  <%= link_to 'Nueva Propiedad', new_realty_path %>
