<br>
<table id="myTable" class="table">
  <thead>
    <tr>
      <th>Seleccionar Tribunal</th>
      <th>Rut</th>
      <th>Dirección</th>
      <th>Teléfono</th>
      <th>Nº Cuenta</th>
      <th>Opciones</th>
    </tr>
  </thead>




  <tbody>
    <% @courts.each do |court| %>

      <tr>
        <td><%= court.name %></td>
        <td><%= court.rut %></td>
        <td><%= court.address %></td>
        <td><%= court.phone %></td>
        <td><%= court.account %></td>
        <td> <%= link_to 'Show', court %>
        <%= link_to 'Edit', edit_court_path(court) %>
        <%= link_to 'Destroy', court, method: :delete, data: { confirm: 'Are you sure?' } %>
</td>


      </tr>
    <% end %>
  </tbody>
</table>

<br>

  <%= link_to 'Nuevo Tribunal', new_court_path %>
