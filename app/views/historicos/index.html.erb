<p id="notice"><%= notice %></p>

<h1>Historicos</h1>

<table>
  <thead>
    <tr>
      <th>Persona</th>
      <th>Type member</th>
      <th>Legal persona</th>
      <th>Entrada</th>
      <th>Salida</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @historicos.each do |historico| %>
      <tr>
        <td><%= historico.persona_id %></td>
        <td><%= historico.type_member %></td>
        <td><%= historico.legal_persona_id %></td>
        <td><%= historico.entrada %></td>
        <td><%= historico.salida %></td>
        <td><%= link_to 'Show', historico %></td>
        <td><%= link_to 'Edit', edit_historico_path(historico) %></td>
        <td><%= link_to 'Destroy', historico, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Historico', new_historico_path %>
