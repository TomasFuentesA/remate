
<%- if controller_name != 'sessions' %>
<div class="form-group">
  <%= link_to "Ingresar", new_session_path(resource_name) %><br />
  </div>
<% end %>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
<div class="form-group">
  <%= link_to "Registrate", new_registration_path(resource_name) %><br />
  </div>
<% end %>

<%- if devise_mapping.recoverable? && controller_name != 'passwords' && controller_name != 'registrations' %>
<div class="form-group">
  <%= link_to "Olvid칩 su contrase침a?", new_password_path(resource_name) %><br />
  </div>
<% end %>

<%- if devise_mapping.confirmable? && controller_name != 'confirmations' %>
<div class="form-group">
  <%= link_to "No ha recibido las instrucciones de confirmaci칩n?", new_confirmation_path(resource_name) %><br />
  </div>
<% end %>

<%- if devise_mapping.lockable? && resource_class.unlock_strategy_enabled?(:email) && controller_name != 'unlocks' %>
<div class="form-group">
  <%= link_to "No ha recibido las instrucciones de desbloqueo?", new_unlock_path(resource_name) %><br />
  </div>
<% end %>

<%- if devise_mapping.omniauthable? %>
<div class="form-group">
  <%- resource_class.omniauth_providers.each do |provider| %>
    <%= link_to "Iniciar sesi칩n con: #{OmniAuth::Utils.camelize(provider)}", omniauth_authorize_path(resource_name, provider)%><br />
  <% end %>
  </div>
<% end %>
