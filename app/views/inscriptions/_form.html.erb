
<br>
<div class="container-fluid" id="wrapper" >
<%= form_with model: @inscription do |form| %>


  <div class="field">
    <%= form.hidden_field :domain_id, value: @domain.id %>
  </div>
  <div class="field">
    <%= form.label :foja, "foja:" %>
    <%= form.number_field :foja%>
  </div>
  <div class="field">
    <%= form.label :number, "número:" %>
    <%= form.number_field :number%>
  </div>

  <div class="field">
    <%= form.label :year, "año:" %>
    <%= form.number_field :year%>
  </div>
  <div class="field">
    <%= form.label :cbrs, "cbrs:" %>
    <%= form.text_field :cbrs %>
  </div>
  <div class="field">
    <%= form.label :date, "Fecha" %>
    <%= form.text_field :date ,id: :date, onchange: "calculo_uf(this)", data:{provide:'datepicker',date_format: "dd-mm-yyyy"},autocomplete: "off" %>
  </div>

 <%= form.submit "Guardar" %>

<% end %>
</div>
