<div class="container">
   <%= render 'errors' %>
   <%= link_to  auctionlistings_path, class: "btn btn-primary" do %>
  <%= fa_icon "backspace" %>volver Listado Remates
  <% end %>
  <div class="row justify-content-md-center">
    <div class="card" style="width: 40%; ">
      <div class="card-body">
        <%= form_with model: @auctionlisting do |form| %>
        <div class="form-group">
            <h2>Nombre: <%= @auctionlisting.name %></h2>
        </div>
        <div class="form-group">
            <p>Archivo: <%= @auctionlisting.attached %></p>
        </div>
        <% end %>
      </div>
    </div>
  </div>
</div>

 