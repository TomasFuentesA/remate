<h1>NaturalPeople#index</h1>
<p>Find me in app/views/natural_people/index.html.erb</p>



<h1>Personas Naturales</h1>
<div class="table-responsive">
<table>
  <thead>
    <tr>

      <th>RUt</th>
      <th>Nombre</th>

    </tr>
  </thead>

  <tbody>
    <% @naturalPeople.each do |naturalPerson| %>
      <tr>

        <td><%= naturalPerson.rut %></td>
        <td><%= naturalPerson.name %></td>

      </tr>
    <% end %>
  </tbody>
</table>
</div>
<br>
